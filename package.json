  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1"
  },
  "browserslist": [
    "last 6 version",
    "> 1%",
    "IE 10"
  ],
  "bugs": {
